{
  "name": "@evidnet/scv-core",
  "version": "1.1.1",
  "description": "Command-line Helper for generate multiple docker images with templates.",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "test": "ava -v",
    "compile": "tsc",
    "prepublish": "npm run compile",
    "lint": "tslint \"src/**/*.ts\" \"test/**/*.ts\"",
    "build": "npm-run-all compile lint test"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evidnet/scv-core.git"
  },
  "keywords": [
    "cli",
    "docker",
    "template"
  ],
  "author": "Haze Lee <hazelee@evidnet.co.kr>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/evidnet/scv-core/issues"
  },
  "homepage": "https://github.com/evidnet/scv-core#readme",
  "devDependencies": {
    "ava": "^1.0.0-rc.1",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.3",
    "rxjs-tslint": "^0.1.5",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.1.3"
  },
  "dependencies": {
    "@types/dockerode": "^2.5.5",
    "@types/node": "^10.12.0",
    "@types/node-fetch": "^2.1.2",
    "@types/rimraf": "^2.0.2",
    "caporal": "^0.10.0",
    "dockerode": "^2.5.7",
    "node-fetch": "^2.2.0",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  }
}
